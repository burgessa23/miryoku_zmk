// Alt+Tab swapper, requires PR #1366

ZMK_BEHAVIOR(smart_mouse, tri_state,
    bindings = <&tog U_MOUSE>, <&none>, <&tog U_MOUSE>;
    ignored-key-positions = <RT1 RT2 RT3 RM1 RM2 RM3 RM4 RB1 RB2 RB3 RH0 RH1 RH3>;
    ignored-layers = <U_MOUSE>;
)

